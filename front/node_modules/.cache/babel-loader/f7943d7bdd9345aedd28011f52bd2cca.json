KAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,CAAA,CAAA;AACA,OATA;AAUA,KAZA;AAcA,IAAA,WAdA,yBAcA;AACA,WAAA,iBAAA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,CAAA,CAAA;AACA,KAlBA;AAoBA,IAAA,iBApBA,6BAoBA,QApBA,EAoBA,KApBA,EAoBA;AACA,WAAA,eAAA,GAAA,QAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAvBA;AAwBA,IAAA,WAxBA,yBAwBA;AAAA;;AACA,MAAA,mBAAA,CAAA,WAAA,CAAA,KAAA,KAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,CAAA,CAAA;;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA;AACA,OATA;AAUA,KAnCA;AAoCA,IAAA,SApCA,uBAoCA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAtCA;AAuCA,IAAA,UAvCA,wBAuCA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,WAAA;AACA;AA1CA,GAhBA;AA4DA,EAAA,OA5DA,qBA4DA;AACA,SAAA,iBAAA;AACA;AA9DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-12\">\r\n      <div class=\"input-group mb-4\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          placeholder=\"Buscar por titulo\"\r\n          v-model=\"title\"\r\n        />\r\n\r\n        <div class=\"input-group-append\">\r\n          <button\r\n            class=\"btn btn-outline-primary\"\r\n            type=\"button\"\r\n            @click=\"searchTitle\"\r\n          >\r\n            Buscar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Lista de Tutoriales</h4>\r\n      <hr />\r\n      <p v-if=\"!tutorials\">No se encontraron tutoriales!</p>\r\n      <p v-else-if=\"isEmpty\">No hay tutoriales</p>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"list-group-item\"\r\n          :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(tutorial, index) in tutorials\"\r\n          :key=\"index\"\r\n          @click=\"setActiveTutorial(tutorial, index)\"\r\n        >\r\n          {{ tutorial.title }}\r\n        </li>\r\n      </ul>\r\n      <br />\r\n\r\n      <Modal v-show=\"isModalVisible\" @close=\"closeModal\">\r\n        <template v-slot:header> Eliminar Todos </template>\r\n\r\n        <template v-slot:body\r\n          >Esta seguro que quiere eliminar todos los tutoriales?</template\r\n        >\r\n\r\n        <template v-slot:footer> Seran eliminados permanentemente!</template> />\r\n      </Modal>\r\n      <button\r\n        v-if=\"!search && !isEmpty\"\r\n        class=\"btn btn-sm btn-danger float-right\"\r\n        @click=\"showModal\"\r\n      >\r\n        Eliminar Todos\r\n      </button>\r\n      <button v-else-if=\"search\"\r\n      class=\"btn btn-sm btn-danger\"\r\n        @click=\"retrieveTutorials\"\r\n      >\r\n        Limpiar busqueda\r\n      </button>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Tutorial</h4>\r\n      <hr />\r\n      <div v-if=\"currentTutorial\">\r\n        <div>\r\n          <label><strong>Titulo:</strong></label> {{ currentTutorial.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Descripcion:</strong></label>\r\n          {{ currentTutorial.description }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Estado:</strong></label>\r\n          {{ currentTutorial.published ? \"Publicado\" : \"Pendiente\" }}\r\n        </div>\r\n        <router-link\r\n          :to=\"`/tutorials/${currentTutorial.id}`\"\r\n          class=\"btn btn-warning\"\r\n          >Editar</router-link\r\n        >\r\n      </div>\r\n      <div v-else>\r\n        <p>Haz click en un tutorial...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TutorialDataService from \"../services/TutorialDataService\";\r\nimport Modal from \"./Modal\";\r\n\r\nexport default {\r\n  components: {\r\n    Modal,\r\n  },\r\n  name: \"tutorials-list\",\r\n  data() {\r\n    return {\r\n      tutorials: [],\r\n      currentTutorial: null,\r\n      currentIndex: -1,\r\n      title: \"\",\r\n      search: undefined,\r\n      isEmpty: undefined,\r\n      isModalVisible: false,\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveTutorials() {\r\n      TutorialDataService.getAll().then((response) => {\r\n        if (response.data.length == 0) {\r\n          this.isEmpty = true;\r\n        }\r\n        this.tutorials = response.data;\r\n        this.title = \"\";\r\n        this.search = false;\r\n        this.currentTutorial = null;\r\n        this.currentIndex = -1;\r\n      });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrieveTutorials();\r\n      this.currentTutorial = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveTutorial(tutorial, index) {\r\n      this.currentTutorial = tutorial;\r\n      this.currentIndex = index;\r\n    },\r\n    searchTitle() {\r\n      TutorialDataService.findByTitle(this.title).then((response) => {\r\n        this.currentTutorial = null;\r\n        this.search = true;\r\n        this.currentIndex = -1;\r\n        if (response.data.length == 0) {\r\n          this.tutorials = null;\r\n        } else {\r\n          this.tutorials = response.data;\r\n        }\r\n      });\r\n    },\r\n    showModal() {\r\n      this.isModalVisible = true;\r\n    },\r\n    closeModal() {\r\n      this.isModalVisible = false;\r\n      this.refreshList();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveTutorials();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}