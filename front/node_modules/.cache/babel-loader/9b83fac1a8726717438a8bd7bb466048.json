          console.log(e);\r\n        });\r\n    },\r\n    \r\n    newTutorial() {\r\n      this.submitted = false;\r\n      this.tutorial = {};\r\n    },\r\n\r\n    goHome() {\r\n        this.$router.push(\"/tutorials\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}