A,IAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,eAAA,CAAA,SAAA,GAAA,MAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAHA;AAIA,KAnBA;AAqBA,IAAA,cArBA,4BAqBA;AAAA;;AACA,UAAA,CAAA,KAAA,eAAA,CAAA,KAAA,EAAA;AACA,aAAA,OAAA,GAAA,kBAAA;AACA,OAFA,MAEA;AACA,QAAA,mBAAA,CAAA,MAAA,CACA,KAAA,eAAA,CAAA,EADA,EAEA,KAAA,eAFA,EAGA,IAHA,CAGA,YAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,yCAAA;AACA,SALA;AAMA;AACA,KAhCA;AAiCA,IAAA,SAjCA,uBAiCA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAnCA;AAoCA,IAAA,UApCA,wBAoCA;AACA,WAAA,cAAA,GAAA,KAAA;AACA;AAtCA,GAbA;AAqDA,EAAA,OArDA,qBAqDA;AACA,SAAA,OAAA,GAAA,EAAA;AACA,SAAA,WAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA;AAxDA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"currentTutorial\" class=\"edit-form  \">\r\n    <h4>Tutorial</h4>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"title\">Titulo</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"title\"\r\n          v-model=\"currentTutorial.title\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">Descripcion</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"description\"\r\n          v-model=\"currentTutorial.description\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label><strong>Estado:</strong></label>\r\n        {{ currentTutorial.published ? \"Publicado\" : \"Pendiente\" }}\r\n        <p style=\"color:red;margin-top:15px\">{{ message }}</p>\r\n      </div>\r\n    </form>\r\n    <div style=\"margin: auto; display: block;\">\r\n      <button\r\n        class=\"btn btn-secondary m-2\" \r\n        v-if=\"currentTutorial.published\"\r\n        @click=\"updatePublished(false)\"\r\n      >\r\n        Pendiente\r\n      </button>\r\n      <button\r\n        v-else\r\n        class=\"btn btn-primary m-2\"\r\n        @click=\"updatePublished(true)\"\r\n      >\r\n        Publicar\r\n      </button>\r\n      <button type=\"button\" class=\"btn btn-danger m-2\" @click=\"showModal\">\r\n        Eliminar\r\n      </button>\r\n\r\n      <Modal\r\n        v-show=\"isModalVisible\"\r\n        :currentTutorialId=\"currentTutorial.id\"\r\n        @close=\"closeModal\"\r\n      >\r\n        <template v-slot:header> Eliminar </template>\r\n\r\n        <template v-slot:body\r\n          >Esta seguro que desea eliminar el tutorial?</template\r\n        >\r\n\r\n        <template v-slot:footer> Sera eliminado permanentemente!</template> />\r\n      </Modal>\r\n\r\n      <button type=\"submit\" class=\"btn btn-success m-2\" @click=\"updateTutorial\">\r\n        Actualizar\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TutorialDataService from \"../services/TutorialDataService\";\r\nimport Modal from \"./Modal\";\r\n\r\nexport default {\r\n  components: {\r\n    Modal,\r\n  },\r\n  name: \"tutorial\",\r\n  data() {\r\n    return {\r\n      currentTutorial: null,\r\n      message: \"\",\r\n      isModalVisible: false,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getTutorial(id) {\r\n      TutorialDataService.get(id).then((response) => {\r\n        this.currentTutorial = response.data;\r\n      });\r\n    },\r\n\r\n    updatePublished(status) {\r\n      var data = {\r\n        id: this.currentTutorial.id,\r\n        title: this.currentTutorial.title,\r\n        description: this.currentTutorial.description,\r\n        published: status,\r\n      };\r\n\r\n      TutorialDataService.update(this.currentTutorial.id, data).then(() => {\r\n        this.currentTutorial.published = status;\r\n        this.message = \"\";\r\n      });\r\n    },\r\n\r\n    updateTutorial() {\r\n      if (!this.currentTutorial.title) {\r\n        this.message = \"Titulo requerido\";\r\n      } else {\r\n        TutorialDataService.update(\r\n          this.currentTutorial.id,\r\n          this.currentTutorial\r\n        ).then(() => {\r\n          this.message = \"El tutorial se actualizo correctamente!\";\r\n        });\r\n      }\r\n    },\r\n    showModal() {\r\n      this.isModalVisible = true;\r\n    },\r\n    closeModal() {\r\n      this.isModalVisible = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.message = \"\";\r\n    this.getTutorial(this.$route.params.id);\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.edit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n.btn {\r\n  padding:.500rem .500rem;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}